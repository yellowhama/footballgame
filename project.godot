[debug]

file_logging/enable_file_logging=true
file_logging/max_log_files=50
gdscript/warnings/treat_warnings_as_errors=false

[dialogic]

directories/dch_directory={
"manager": "res://dialogic/characters/manager.dch"
}
directories/dtl_directory={
"manager_first_meeting": "res://dialogic/timelines/manager_first_meeting.dtl"
}
layout/style_directory={}

[display]

window/size/viewport_width=1080
window/size/viewport_height=1920
window/size/window_width_override=1080
window/size/window_height_override=1920
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_width"
window/handheld/orientation=1

[editor_plugins]

enabled=PackedStringArray("res://addons/event_bus_plugin/plugin.cfg")

[filesystem]

import/blender/enabled=false

[game]

use_binary_timeline=true

[gdscript]

; Disable treating type inference warnings as errors (headless CI must not fail on inference warnings).
warn/inferred_declaration=0
warn/unsafe_property_access=0
warn/unsafe_method_access=0
warn/unsafe_cast=0
warn/unsafe_call_argument=0
warn/return_value_discarded=0
warn/narrowing_conversion=0


[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = "4.4"
reloadable = false

[libraries]
linux.debug.x86_64 = "res://target/debug/libgodot_bridge.so"
linux.release.x86_64 = "res://target/release/libgodot_bridge.so"

windows.debug.x86_64 = "res://target/debug/godot_bridge.dll"
windows.release.x86_64 = "res://target/release/godot_bridge.dll"

macos.debug = "res://target/debug/libfootball_rust.dylib"
macos.debug.arm64 = "res://target/debug/libfootball_rust.dylib"
macos.release = "res://target/release/libfootball_rust.dylib"
macos.release.arm64 = "res://target/release/libfootball_rust.dylib"

android.debug.arm64 = "res://godot_extension/target/android/arm64-v8a/libfootball_rust.so"
android.release.arm64 = "res://godot_extension/target/android/arm64-v8a/libfootball_rust.so"
android.debug.arm32 = "res://godot_extension/target/android/armeabi-v7a/libfootball_rust.so"
android.release.arm32 = "res://godot_extension/target/android/armeabi-v7a/libfootball_rust.so"
android.debug.x86_64 = "res://godot_extension/target/android/x86_64/libfootball_rust.so"
android.release.x86_64 = "res://godot_extension/target/android/x86_64/libfootball_rust.so"

ios.debug = "res://godot_extension/target/ios/libfootball_rust.xcframework"
ios.release = "res://godot_extension/target/ios/libfootball_rust.xcframework"